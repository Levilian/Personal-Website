{
  "name": "levi-lian-personal-website",
  "description": "Levi Lian's Personal Website",
  "version": "1.0.0",
  "author": "Levi Lian <levilian@stanford.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Levilian/Personal-Website"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve"
