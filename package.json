    "styled-components": "^6.0.7",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@upstatement/eslint-config": "2.0.0",
    "@upstatement/prettier-config": "^1.1.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.x.x",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.x.x",
    "pretty-quick": "^3.1.3"
